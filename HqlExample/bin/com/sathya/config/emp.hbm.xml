<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.sathya.entity.Employee" table="emp">
  
    <id name="empNumber" column="eno" type="int"/>
    <property name="empName" column="ename" type="java.lang.String"/>
    <property name="empsal" column="esal" type="int" />
    <property name="deptNumber" column="deptno" type="int"/>

  </class>

</hibernate-mapping>